generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Todo {
  id        String    @id @default(uuid())
  title     String
  userId    String
  createdAt DateTime  @default(now())
  deletedAt DateTime? @db.Timestamptz(6)

  @@index([userId])
}
